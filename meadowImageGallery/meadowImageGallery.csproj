<Project Sdk="Meadow.Sdk/1.1.0">
  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <CopyLocalLockFileAssemblies>true</CopyLocalLockFileAssemblies>
    <OutputType>Exe</OutputType>
    <AssemblyName>App</AssemblyName>
  </PropertyGroup>
  <ItemGroup>
    <None Remove="image1.jpg" />
    <None Remove="image2.jpg" />
    <None Remove="image3.jpg" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="image1.jpg" />
    <EmbeddedResource Include="image2.jpg" />
    <EmbeddedResource Include="image3.jpg" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Meadow.F7" Version="0.*" />
    <PackageReference Include="Meadow.Foundation" Version="0.*" />
    <PackageReference Include="Meadow.Foundation.Displays.TftSpi" Version="0.7.47" />
    <PackageReference Include="SimpleJpegDecoder" Version="0.4.0" />
  </ItemGroup>
</Project>
